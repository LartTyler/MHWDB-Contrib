{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-eslint-rules"
	],
	"jsRules": {},
	"rules": {
		"arrow-parens": {
			"options": [
				"ban-single-arg-parens"
			]
		},
		"curly": false,
		"indent": {
			"options": [
				"tabs",
				4
			]
		},
		"jsx-no-multiline-js": false,
		"jsx-no-lambda": false,
		"object-curly-spacing": {
			"options": [
				true
			]
		},
		"quotemark": {
			"options": [
				"single",
				"avoid-escape",
				"jsx-double"
			]
		},
		"semicolon": {
			"options": [
				"always",
				"strict-bound-class-methods"
			]
		},
		"trailing-comma": {
			"options": [
				{
					"multiline": "always",
					"esSpecCompliant": false
				}
			]
		},
		"member-ordering": [
			true,
			{
				"order": "instance-sandwich"
			}
		],
		"object-literal-key-quotes": [
			true,
			"as-needed"
		]
	}
}
